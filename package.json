{
  "name": "rollup-plugin-compressor",
  "version": "1.0.5",
  "description": "Compress dist folder",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "require": "./dist/index.cjs.js",
    "import": "./dist/index.esm.js",
    "types": "./dist/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JY-Mar/rollup-plugin-compressor.git"
  },
  "scripts": {
    "dev": "tsc -w -p .",
    "build": "rimraf dist && tsc -p . && npm link",
    "rollup:build": "rimraf dist && rollup -c && npm link"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "compressor",
    "scientificat1995"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^24.3.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.21.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "compressing": "^1.5.1"
  }
}
