{
  "name": "rollup-plugin-compressor",
  "version": "1.0.2",
  "description": "Compress dist folder",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JY-Mar/rollup-plugin-compressor.git"
  },
  "scripts": {
    "dev": "tsc -w -p .",
    "build": "rimraf dist && tsc -p . && npm link",
    "rollup:build": "rimraf dist && rollup -c && npm link"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "compressor",
    "scientificat1995"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.21.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "compressing": "^1.5.1"
  }
}
